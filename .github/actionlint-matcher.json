{
  "problemMatcher": [
    {
      "owner": "actionlint-detailed",
      "pattern": [
        {
          "regexp": "^::(?:(?:warning)|(?:error)) file=(.+),line=(\\d+),col=(\\d+)::(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "actionlint-brief",
      "pattern": [
        {
          "regexp": "^::(?:(?:warning)|(?:error)) file=(.+),line=(\\d+)::(.*)$",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    }
  ]
}
